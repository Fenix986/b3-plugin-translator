<configuration plugin="translator">
    <settings name="settings">
        <set name="default_source_language"></set>       <!-- default source language code (leave empty for language auto detection) -->
        <set name="default_target_language">en</set>     <!-- default target language code (if empty will fallback to English) -->
        <set name="display_translator_name">True</set>   <!-- whether to display the translator service as translation prefix -->
        <set name="translator_name">^7[^1T^7] </set>     <!-- a name with which to prefix translated messages -->
        <set name="message_prefix">^3</set>              <!-- a color code with which to prefix translated messages -->
        <set name="min_sentence_length">6</set>          <!-- minimum length of a message to be translated using !transauto and !translast  -->
        <set name="microsoft_client_id"></set>           <!-- Microsoft translator client id -->
        <set name="microsoft_client_secret"></set>       <!-- microsoft translator client secret -->
    </settings>
    <settings name="commands">
        <set name="translate">2</set>
        <set name="translast">2</set>
        <set name="transauto">80</set>
        <set name="translang">2</set>
    </settings>
</configuration>